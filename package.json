{
  "name": "smart-resume-analyzer",
  "version": "1.0.0",
  "description": "AI-powered resume analysis application with FastAPI backend and Next.js frontend",
  "scripts": {
    "dev": "concurrently \"cd backend && bash -c 'source .venv/bin/activate && uvicorn app.main:app --reload'\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "lint": "cd frontend && npm run lint",
    "test": "cd backend && pytest",
    "setup": "./scripts/setup.sh",
    "logs": "tail -f backend/app-logs/app.log",
    "logs:clear": "rm -f backend/app-logs/*.log && echo 'Application logs cleared'"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "ai",
    "resume-analyzer",
    "fastapi",
    "nextjs",
    "langchain",
    "openai"
  ],
  "author": {
    "name": "Bryan Thompson",
    "email": "bryan@triepod.ai"
  },
  "license": "MIT",
  "private": true
}